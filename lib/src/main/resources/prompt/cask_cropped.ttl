                                                                rdf:rest [ rdf:type rdf:List ;
                                                                           rdf:first "NOR" ;
                                                                           rdf:rest [ rdf:type rdf:List ;
                                                                                      rdf:first "OR" ;
                                                                                      rdf:rest [ rdf:type rdf:List ;
                                                                                                 rdf:first "XOR" ;
                                                                                                 rdf:rest rdf:nil
                                                                                               ]
                                                                                    ]
                                                                         ]
                                                              ]
                                                   ]
                                       ] ;
                            rdfs:comment "Is used with Boolean_Association to define what kind of boolean relation connects the Data_Elements of the Boolean_Association" .


