<http://openmath.org/vocab/math#Binding> rdf:type owl:Class ;
                                         rdfs:subClassOf <http://openmath.org/vocab/math#Compound> ,
                                                         [ rdf:type owl:Restriction ;
                                                           owl:onProperty <http://openmath.org/vocab/math#binder> ;
                                                           owl:allValuesFrom <http://openmath.org/vocab/math#Object>
                                                         ] ,
                                                         [ rdf:type owl:Restriction ;
                                                           owl:onProperty <http://openmath.org/vocab/math#body> ;
                                                           owl:allValuesFrom <http://openmath.org/vocab/math#Object>
                                                         ] ,
                                                         [ rdf:type owl:Restriction ;
                                                           owl:onProperty <http://openmath.org/vocab/math#variables> ;
                                                           owl:allValuesFrom <http://openmath.org/vocab/math#VariableList>
                                                         ] ,
                                                         [ rdf:type owl:Restriction ;
                                                           owl:onProperty <http://openmath.org/vocab/math#binder> ;
                                                           owl:cardinality "1"^^xsd:nonNegativeInteger
                                                         ] ,
                                                         [ rdf:type owl:Restriction ;
                                                           owl:onProperty <http://openmath.org/vocab/math#body> ;
                                                           owl:cardinality "1"^^xsd:nonNegativeInteger
                                                         ] ,
                                                         [ rdf:type owl:Restriction ;
                                                           owl:onProperty <http://openmath.org/vocab/math#variables> ;
                                                           owl:maxCardinality "1"^^xsd:nonNegativeInteger
                                                         ] ;
